{
	"_version": "1.12.0",
	"sap.app": {
		"id": "harry",
		"type": "application"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "harry.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "harry.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/todoitems.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
	  	"routing": {
		  "config": {
			"routerClass": "sap.m.routing.Router",
			"controlId": "appControl",
			"viewType": "XML",
			"controlAggregation": "pages",
			"viewPath": "harry.view",
			"async": true,
			"bypassed": {
			  "target": "login"
			}
		  },
		  "routes": [
			{
			  "pattern": "",
			  "name": "index",
			  "target": "login"
			},
			{
			  "pattern": "login",
			  "name": "login",
			  "target": "login"
			},
			{
			  "pattern": "todo",
			  "name": "todo",
			  "target": "todo"
			}
		  ],
		  "targets": {
			"todo": {
			  "viewId": "todo",
			  "viewName": "Todo"
			},
			"login": {
			  "viewId": "login",
			  "viewName": "login.Login"
			}
		  }
		}
	}
}
