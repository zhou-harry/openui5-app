{
  "name": "openui5-app",
  "version": "0.2.0",
  "description": "OpenUI5 app",
  "private": true,
  "scripts": {
    "start": "ui5 serve",
    "debug": "ui5 %NODE_DEBUG_OPTION% serve --port=8087",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "watch": "start-server-and-test start http://localhost:8080 karma",
    "test": "npm run lint && rimraf coverage && start-server-and-test start http://localhost:8080 karma-ci",
    "build": "rimraf dist && ui5 build --a"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.60.0",
    "@openui5/sap.tnt": "^1.60.0",
    "@openui5/sap.f": "^1.60.0",
    "@openui5/sap.uxap": "^1.60.0",
    "@openui5/sap.ui.core": "^1.60.0",
    "@openui5/sap.ui.codeeditor": "^1.60.0",
    "@openui5/sap.ui.commons": "^1.60.0",
    "@openui5/sap.ui.demokit": "^1.60.0",
    "@openui5/sap.ui.documentation": "^1.60.0",
    "@openui5/sap.ui.dt": "^1.60.0",
    "@openui5/sap.ui.fl": "^1.60.0",
    "@openui5/sap.ui.layout": "^1.60.0",
    "@openui5/sap.ui.rta": "^1.60.0",
    "@openui5/sap.ui.suite": "^1.60.0",
    "@openui5/sap.ui.support": "^1.60.0",
    "@openui5/sap.ui.table": "^1.60.0",
    "@openui5/sap.ui.unified": "^1.60.0",
    "@openui5/sap.ui.ux3": "^1.60.0",
    "@openui5/themelib_sap_bluecrystal": "^1.60.0",
    "@openui5/themelib_sap_belize": "^1.60.0"
  },
  "devDependencies": {
    "@ui5/cli": "^1.0.0",
    "eslint": "^4.19.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-openui5": "^0.2.3",
    "karma-qunit": "^1.2.1",
    "qunitjs": "^2.4.1",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.4.1"
  }
}
